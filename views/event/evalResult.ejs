<body>

    <br>

    <h1><svg width="1.5em" height="1.5em" viewBox="0 0 16 16" class="bi bi-book text-primary" fill="currentColor"
            xmlns="http://www.w3.org/2000/svg">
            <path fill-rule="evenodd"
                d="M1 2.828v9.923c.918-.35 2.107-.692 3.287-.81 1.094-.111 2.278-.039 3.213.492V2.687c-.654-.689-1.782-.886-3.112-.752-1.234.124-2.503.523-3.388.893zm7.5-.141v9.746c.935-.53 2.12-.603 3.213-.493 1.18.12 2.37.461 3.287.811V2.828c-.885-.37-2.154-.769-3.388-.893-1.33-.134-2.458.063-3.112.752zM8 1.783C7.015.936 5.587.81 4.287.94c-1.514.153-3.042.672-3.994 1.105A.5.5 0 0 0 0 2.5v11a.5.5 0 0 0 .707.455c.882-.4 2.303-.881 3.68-1.02 1.409-.142 2.59.087 3.223.877a.5.5 0 0 0 .78 0c.633-.79 1.814-1.019 3.222-.877 1.378.139 2.8.62 3.681 1.02A.5.5 0 0 0 16 13.5v-11a.5.5 0 0 0-.293-.455c-.952-.433-2.48-.952-3.994-1.105C10.413.809 8.985.936 8 1.783z" />
        </svg>
        &nbsp;<%= sectioninfo.in[0].courseID %>: <%= sectioninfo.in[0].courseName %> (Section <%= sectioninfo.sectionNum
                    %>)
    </h1>

    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/homepage/<%= userid %>">Home</a></li>
            <li class="breadcrumb-item"><a href="/student/<%= userid %>/viewProject/<%= sectioninfo.id %>">
                    <%= sectioninfo.in[0].courseID %>: <%= sectioninfo.in[0].courseName %> (Section <%=
                                sectioninfo.sectionNum %>)
                </a></li>
            <li class="breadcrumb-item"><a
                    href="/student/section/<%= sectioninfo.id %>/project/<%= sectioninfo.haveProject[0].id %>/evaluationEvent">
                    <%= sectioninfo.haveProject[0].projectName %>
                </a></li>
            <li class="breadcrumb-item active" aria-current="page">
                <%= evalFormInfo.eventName %> - Result
            </li>
        </ol>
    </nav>

    <br>

    <h3><svg xmlns="http://www.w3.org/2000/svg" width="1.4em" height="1.4em" fill="currentColor"
            class="bi bi-check2-square text-success" viewBox="0 0 16 16">
            <path
                d="M3 14.5A1.5 1.5 0 0 1 1.5 13V3A1.5 1.5 0 0 1 3 1.5h8a.5.5 0 0 1 0 1H3a.5.5 0 0 0-.5.5v10a.5.5 0 0 0 .5.5h10a.5.5 0 0 0 .5-.5V8a.5.5 0 0 1 1 0v5a1.5 1.5 0 0 1-1.5 1.5H3z" />
            <path
                d="M8.354 10.354l7-7a.5.5 0 0 0-.708-.708L8 9.293 5.354 6.646a.5.5 0 1 0-.708.708l3 3a.5.5 0 0 0 .708 0z" />
        </svg> &nbsp;Submitted Evaluation > Results > Group <%= members.groupNum %>
    </h3>



    <div class="alert alert-success alert-dismissible fade show" role="alert">
        Peer evaluation result has been saved to your profile by default.
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>

    <br>

    <h4><mark><svg xmlns="http://www.w3.org/2000/svg" width="35" height="35" fill="currentColor"
                class="bi bi-people-fill text-danger" viewBox="0 0 16 16">
                <path d="M7 14s-1 0-1-1 1-4 5-4 5 3 5 4-1 1-1 1H7zm4-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z" />
                <path fill-rule="evenodd"
                    d="M5.216 14A2.238 2.238 0 0 1 5 13c0-1.355.68-2.75 1.936-3.72A6.325 6.325 0 0 0 5 9c-4 0-5 3-5 4s1 1 1 1h4.216z" />
                <path d="M4.5 8a2.5 2.5 0 1 0 0-5 2.5 2.5 0 0 0 0 5z" />
            </svg> &nbsp;Peer Evaluation Question</mark></h4>

    <% var n=0; %>

        <% evalFormInfo.evaluationTemp.peerEvaluation.forEach( function(q) { %>

            <div class="my-4" style="padding-left: 45px;">

                <div class="my-3"><b><u>Q<%= n+1 %>. <%= q.question %></u></b></div>

                <div style="padding-left: 33px;">

                    <% evalFormInfo.completedResponse.forEach( function(c) { %>

                        <% c.formResponse.peerEvaluation.forEach( function(r) { %>

                            <% if(r.evaluatee==userid) { %>

                                <div class="my-2" style="padding-left: 38px;">
                                    <figure class="text-start">
                                        <blockquote class="blockquote">
                                            <p>
                                                <%= r.response[n].answer %>
                                            </p>
                                        </blockquote>
                                        <figcaption class="blockquote-footer">
                                            <!-- Anonymous feeback -->
                                            <cite title="Source Title">
                                                Anonymous feeback
                                                <!-- <%= sectioninfo.haveProject[0].projectName %> - (Group <%=
                                                        members.groupNum %>) -->
                                            </cite>
                                        </figcaption>
                                    </figure>

                                </div>

                                <% } %>



                                    <% }); %>

                                        <% }); %>


                </div>




                <br>

                <% n++ %>

            </div>


            <% }); %>


</body>

<style>
    b {
        color: #324880;
        font-size: large;
    }
</style>