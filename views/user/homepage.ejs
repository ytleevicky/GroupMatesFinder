    <h3>Upcoming Events</h3>

    <br>

    <div class="row">

        <div class="col-sm-4">

            <div class="card bg-light mb-3" style="max-width: 18rem;">

                <div class="card-body">
                    <h5 class="card-title">Event 1</h5>
                    <p class="card-text">Some quick example text to build on the card title and make up the bulk of the
                        card's
                        content.</p>
                </div>
                <div class="card-header">Header</div>
            </div>

        </div>

        <div class="col-sm-4">

            <div class="card bg-light mb-3" style="max-width: 18rem;">

                <div class="card-body">
                    <h5 class="card-title">Event 2</h5>
                    <p class="card-text">Some quick example text to build on the card title and make up the bulk of the
                        card's
                        content.</p>
                </div>
                <div class="card-header">Header</div>
            </div>

        </div>

        <div class="col-sm-4">

            <div class="card bg-light mb-3" style="max-width: 18rem;">

                <div class="card-body">
                    <h5 class="card-title">Event 3</h5>
                    <p class="card-text">Some quick example text to build on the card title and make up the bulk of the
                        card's
                        content.</p>
                </div>
                <div class="card-header">Header</div>
            </div>

        </div>

    </div>

    <hr>

    <h3>Courses:</h3>

    <br>

    <% allCourses.forEach( function(course) { %>

        <div class="card mt-2 px-3">
            <div class="card-body ">
                <div class="row">
                    <div class="col-12 ">
                        <h4 class="card-title "><b><%= course.courseID %> - <%= course.courseName %></b></h4>
                    </div>
                    <div class="col">
                        <h6 class="card-subtitle mb-2 text-muted">
                            <p class="card-text text-muted small "> Teacher: <span class="font-weight-bold"><%= course.teachBy[0].preferred_name %></span>
                            </p>
                        </h6>
                    </div>
                </div>
            </div>
        </div>

        <% }); %>




