<body>

    <br>

    <h1><svg width="1.5em" height="1.5em" viewBox="0 0 16 16" class="bi bi-book text-primary" fill="currentColor"
            xmlns="http://www.w3.org/2000/svg">
            <path fill-rule="evenodd"
                d="M1 2.828v9.923c.918-.35 2.107-.692 3.287-.81 1.094-.111 2.278-.039 3.213.492V2.687c-.654-.689-1.782-.886-3.112-.752-1.234.124-2.503.523-3.388.893zm7.5-.141v9.746c.935-.53 2.12-.603 3.213-.493 1.18.12 2.37.461 3.287.811V2.828c-.885-.37-2.154-.769-3.388-.893-1.33-.134-2.458.063-3.112.752zM8 1.783C7.015.936 5.587.81 4.287.94c-1.514.153-3.042.672-3.994 1.105A.5.5 0 0 0 0 2.5v11a.5.5 0 0 0 .707.455c.882-.4 2.303-.881 3.68-1.02 1.409-.142 2.59.087 3.223.877a.5.5 0 0 0 .78 0c.633-.79 1.814-1.019 3.222-.877 1.378.139 2.8.62 3.681 1.02A.5.5 0 0 0 16 13.5v-11a.5.5 0 0 0-.293-.455c-.952-.433-2.48-.952-3.994-1.105C10.413.809 8.985.936 8 1.783z" />
        </svg>
        &nbsp;<%= sectioninfo.in[0].courseID %>: <%= sectioninfo.in[0].courseName %> (Section <%= sectioninfo.sectionNum
                    %>)
    </h1>

    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/teacher/homepage/<%= userid %>">Home</a></li>
            <li class="breadcrumb-item"><a href="/teacher/<%= userid %>/viewCourse/<%= sectioninfo.in[0].id %>">
                    <%= sectioninfo.in[0].courseID %>: <%= sectioninfo.in[0].courseName %>
                </a></li>
            <li class="breadcrumb-item"><a href="/teacher/<%= userid %>/viewSection/<%= sectioninfo.id %>">
                    Section <%= sectioninfo.sectionNum %>
                </a></li>
            <li class="breadcrumb-item"><a
                    href="/teacher/viewSection/<%= sectioninfo.id %>/project/<%= sectioninfo.haveProject[0].id %>/evaluation">
                    Peer Evaluation
                </a></li>

            <li class="breadcrumb-item"><a
                    href="/teacher/viewSection/<%= sectioninfo.id %>/project/<%= sectioninfo.haveProject[0].id %>/event/<%= eventid %>/viewEvaluationResult">
                    Results
                </a></li>

            <li class="breadcrumb-item"><a
                    href="/teacher/viewSection/<%= sectioninfo.id %>/project/<%= sectioninfo.haveProject[0].id %>/event/<%= eventid %>/group/<%= groupInfo.id %>">
                    Group <%= groupInfo.groupNum %>
                </a></li>
            <li class="breadcrumb-item active" aria-current="page">
                <%= evaluatorInfo.fullName %>
            </li>
        </ol>
    </nav>

    <br>


    <h3 class="col-md-12"><svg xmlns="http://www.w3.org/2000/svg" width="1.4em" height="1.4em" fill="currentColor"
            class="bi bi-calendar4-week text-info" viewBox="0 0 16 16">
            <path
                d="M3.5 0a.5.5 0 0 1 .5.5V1h8V.5a.5.5 0 0 1 1 0V1h1a2 2 0 0 1 2 2v11a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V3a2 2 0 0 1 2-2h1V.5a.5.5 0 0 1 .5-.5zM2 2a1 1 0 0 0-1 1v1h14V3a1 1 0 0 0-1-1H2zm13 3H1v9a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V5z" />
            <path
                d="M11 7.5a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-1a.5.5 0 0 1-.5-.5v-1zm-3 0a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-1a.5.5 0 0 1-.5-.5v-1zm-2 3a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-1a.5.5 0 0 1-.5-.5v-1zm-3 0a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-1a.5.5 0 0 1-.5-.5v-1z" />
        </svg> &nbsp;My Evaluation Event > Results > Group <%= groupInfo.groupNum %>
    </h3>

    <br>

    <div class="container" style="padding: 5px 40px;">

        <dl class="row my-2">
            <dt class="col-sm-3">Evaluator</dt>
            <dd class="col-sm-9">
                <%= evaluatorInfo.fullName %> &nbsp; &nbsp;(<%= evaluatorInfo.givenId %>)
            </dd>
        </dl>

        <dl class="row my-2">
            <dt class="col-sm-3">Date Submitted</dt>
            <dd class="col-sm-9">
                <%= new Intl.DateTimeFormat('en-GB', { dateStyle: 'medium' , timeStyle: 'short' , hour12: 'true'
                    }).format(formInfo.createdAt) %>
            </dd>
        </dl>

        <dl class="row my-2">
            <dt class="col-sm-3">Group Number</dt>
            <dd class="col-sm-9">
                Group <%= groupInfo.groupNum %>
            </dd>
        </dl>

        <dl class="row my-2">
            <dt class="col-sm-3">No. of group members</dt>
            <dd class="col-sm-9">
                <%= groupInfo.createdBy.length %>
            </dd>
        </dl>

    </div>


    <br>
    <br>

    <div class="container" style="padding: 10px 70px;">

        <h4><mark><svg xmlns="http://www.w3.org/2000/svg" width="35" height="35" fill="currentColor"
                    class="bi bi-person-fill text-primary" viewBox="0 0 16 16">
                    <path d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z" />
                </svg> &nbsp;Self Evaluation Question:</mark></h4>

        <div class="my-4" style="padding-left: 45px;">

            <% var n=1; %>

                <% formInfo.formResponse.selfEvaluation.forEach( function(form) { %>

                    <div class="my-2"><b><u>Q<%= n %>. <%= form.question %></u></b></div>
                    <div style="padding-left: 33px;">
                        <%= form.answer %>
                    </div>

                    <br>

                    <% n++ %>

                        <% }); %>

        </div>



        <h4><mark><svg xmlns="http://www.w3.org/2000/svg" width="35" height="35" fill="currentColor"
                    class="bi bi-people-fill text-danger" viewBox="0 0 16 16">
                    <path d="M7 14s-1 0-1-1 1-4 5-4 5 3 5 4-1 1-1 1H7zm4-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z" />
                    <path fill-rule="evenodd"
                        d="M5.216 14A2.238 2.238 0 0 1 5 13c0-1.355.68-2.75 1.936-3.72A6.325 6.325 0 0 0 5 9c-4 0-5 3-5 4s1 1 1 1h4.216z" />
                    <path d="M4.5 8a2.5 2.5 0 1 0 0-5 2.5 2.5 0 0 0 0 5z" />
                </svg> &nbsp;Peer Evaluation Question:</mark></h4>

        <div class="my-4" style="padding-left: 45px;">

            <% formInfo.formResponse.peerEvaluation.forEach( function(student) { %>

                <h5 class="my-3">
                    <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor"
                        class="bi bi-person-check-fill text-danger" viewBox="0 0 16 16">
                        <path fill-rule="evenodd"
                            d="M15.854 5.146a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708 0l-1.5-1.5a.5.5 0 0 1 .708-.708L12.5 7.793l2.646-2.647a.5.5 0 0 1 .708 0z" />
                        <path d="M1 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H1zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z" />
                    </svg>&nbsp;
                    Evaluatee: <%= student.evaluateeFullName %> (<%= student.evaluateeStudentID %>)
                </h5>

                <% var i=1; %>

                    <% student.response.forEach( function(q) { %>

                        <div class="my-2" style="padding-left: 40px;"><b><u>Q<%= i %>. <%= q.question %></u></b></div>

                        <div style="padding-left: 75px;">
                            <%= q.answer %>
                        </div>

                        <br>

                        <% i++ %>

                            <% }); %>


                                <br>


                                <% }); %>

        </div>

    </div>


    <!-- <button class="btn btn-primary">Mark as
                                                Reviewed</button> -->


</body>

<style>
    b {
        color: #324880;
        font-size: large;
    }
</style>